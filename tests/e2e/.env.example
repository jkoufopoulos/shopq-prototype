# MailQ E2E Test Configuration
# Copy this file to .env and fill in your test Gmail credentials

# Gmail Test Account
# IMPORTANT: Use a dedicated test account, NOT your personal Gmail
# Create an App Password: https://myaccount.google.com/apppasswords
GMAIL_TEST_EMAIL=your-test-email@gmail.com
GMAIL_TEST_PASSWORD=your-app-password-here

# Alternative: OAuth Token (if using OAuth instead of password)
# GMAIL_TEST_TOKEN=your-oauth-token-here

# Backend API URL (defaults to localhost:8000)
API_BASE_URL=http://localhost:8000

# Test Configuration
# Set to true to run tests in headed mode (see the browser)
DEBUG=false

# Timeout for individual tests (in milliseconds)
TEST_TIMEOUT=120000
